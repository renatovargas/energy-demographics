@ARTICLE{stern_1986,
  author = {Stern, Paul C.},
  title = {Blind Spots in Policy Analysis: What Economics Doesn't Say about
	Energy Use},
  journal = {Journal of Policy Analysis and Management},
  year = {1986},
  volume = {5},
  pages = {200--227},
  number = {2},
  month = jan,
  note = {{ArticleType:} research-article / Full publication date: Winter,
	1986 / Copyright Â© 1986 Association for Public Policy Analysis and
	Management},
  abstract = {{{\textless}p{\textgreater}This} article describes the difficulties
	of reducing analysis of energy use to an application of economic
	theory. It shows how economic concepts of behavior direct attention
	selectively to some important determinants of consumer behavior and
	away from others; how available economic accounts of short-term change
	in energy use, investment in energy efficiency, and the dynamics
	of investment limit understanding and narrow analysts' vision; how
	promising policy options are overlooked as a result; and how concepts
	and knowledge from the noneconomic behavioral sciences can compensate.
	Two strategies are discussed for improving analysis. Using economic
	theory to guide the improvement of existing models can help, but
	conceptual blind spots will remain. A problem-oriented approach drawing
	on concepts and methods from across the behavioral sciences can avoid
	the blind spots, but cannot be systematic. Analysis can be improved
	by using both approaches in concert; some implications of a combined
	strategy are sketched.},
  doi = {10.2307/3323541},
  issn = {0276-8739},
  shorttitle = {Blind Spots in Policy Analysis},
  url = {http://www.jstor.org/stable/3323541}
}

@ARTICLE{su_2010,
